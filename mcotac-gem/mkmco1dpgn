	pgcc -v -c  -fastsse -tp k8-64 -g -p     wegdat1d_.c
hydro1d_.o: hydro1d_.c  gwheader.h
	pgcc -v -c   -fastsse -tp k8-64 -g -p hydro1d_.c 
gwheader.h: gwheader.h
