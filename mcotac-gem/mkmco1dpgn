mcotac1d.o: mcotac1d.f gwheader.inc kinetics.inc
	pgf90 -v -c    -fastsse -tp k8-64 -g -p  -I/usr/include  -L/usr/lib64 -llammpio -llamf77mpi -lmpi -llam -laio -laio -lutil    mcotac1d.f
datio.o: datio.f gwheader.inc kinetics.inc
	pgf90 -v -c     -fastsse -tp k8-64 -g -p   datio.f
erech.o: erech.f gwheader.inc kinetics.inc
	pgf90 -v -c    -fastsse -tp k8-64 -g  -p    erech.f
