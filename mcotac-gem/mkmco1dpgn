	pgf90 -v -c -fastsse -tp k8-64 -g  -p  inparf.f
setpar_.o: setpar_.c gwheader.h
	pgcc -v -c    -fastsse -tp k8-64 -g -p    setpar_.c
walk2_.o: walk2_.c gwheader.h
	pgcc -v -c  -fastsse -tp k8-64 -g -p     walk2_.c
